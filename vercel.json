{
  "rewrites": [
    { 
      "source": "/((?!zine/).*)", 
      "destination": "/index.html" 
    }
  ],
  "redirects": [
    {
      "source": "/zine/:id",
      "destination": "https://tellmeastory.press",
      "has": [
        {
          "type": "header",
          "key": "Sec-Fetch-Mode",
          "value": "navigate"
        }
      ],
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}