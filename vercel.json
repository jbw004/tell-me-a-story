{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/zine/:id",
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": "^((?!iPhone|iPad|iPod).)*$"
        }
      ],
      "destination": "https://tellmeastory.press",
      "permanent": false
    }
  ]
}